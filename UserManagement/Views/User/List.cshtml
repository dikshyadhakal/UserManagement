@{
  }

@* @model List<UserManagement.Domain.Entities.User> *@

<h1>All Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>ContactNumber</th>
            <th></th>
        </tr>
        <tbody>
            @foreach(var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.Email</td>
                    <td>@user.ContactNumber</td>
                    <td>
                        <a>Edit </a>
                    </td>

                </tr>
            }
        </tbody>
    </thead>
</table>